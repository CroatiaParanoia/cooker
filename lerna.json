{
  "packages": [
    "packages/cookerjs",
    "packages/cookerjs-react"
  ],
  "version": "1.1.0",
  "command": {
    "bootstrap": {
      "hoist": true,
      "npmClientArgs": [
        "--no-package-lock",
        "--no-ci"
      ]
    },
    "version": {
      "allowBranch": [
        "master"
      ],
      "amend": false,
      "conventionalCommits": true
    },
    "publish": {
      "verifyAccess": false,
      "registry": "http://npm.jsany.org"
    }
  }
}
